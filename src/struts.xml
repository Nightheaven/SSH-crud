<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Sofrware Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.i18n.encoding" value="UTF-8"></constant>
	<constant name="struts.objectFactory" value="spring"></constant>
	<package name="basicstruts" extends="struts-default">
		<action name="*Product" class="ProductActionBean" method="{1}">
			<result name="listProduct">listProduct.jsp</result>
			<result name="editProduct">editProduct.jsp</result>
			<result name="listProductAction">listProduct</result>
		</action>
	</package>
</struts>
